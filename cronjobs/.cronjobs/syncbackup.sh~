#!/usr/bin/env sh

echo "-------------- **** Starting Main Backup $(date) **** -------------"

TEMP_FILE=`mktemp --suffix .internalbackup`
touch $TEMP_FILE

rsync --progress -a -r /home/max/ /mnt/Data/LinuxHome/
rsync --progress -a -r /mnt/Data/ /mnt/Backup/


echo "Internal Backup Successful: $(date)" >> /home/max/.cronjobs/backup.log
rm $TEMP_FILE
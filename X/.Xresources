! URXVT
URxvt*borderless:         true
URxvt*internalBorder:     15
URxvt*jumpScroll:         true
URxvt*scrollBar:          false
URxvt*scrollTryOutput:    false
URxvt*scrollTtyKeypress:  true
URxvt*urgentOnBell:       true
URxvt*visualBell:         false
URxvt*letterSpace:        -1
URxvt.depth: 32
!URxvt.background:	[95]#fef1ce

URxvt.font: xft:Hack:size=10,style=regular
URxvt.boldFont: xft:Hack:size=10,style=bold
URxvt.italicFont: xft:Hack:pixelsize=10,style=italic
URxvt.boldItalicFont: xft:Hack:pixelsize=10,style=bold

URxvt.keysym.Control-Shift-Y: perl:clipboard:paste
URxvt.iso14755: False
URxvt.perl-ext-common: default,clipboard


